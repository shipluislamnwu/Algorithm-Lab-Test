#include<iostream>
using namespace std;

int arr[50][50];

int nCr(int n, int k) {
    if (k == 0 || k == n)
        return arr[n][k] = 1;

    if (arr[n][k] != -1)
        return arr[n][k];

    return arr[n][k] = nCr(n-1, k-1) + nCr(n-1, k);
}

int main() {
    int n, k;
    cin >> n >> k;

    for (int i=0; i<50; i++)
        for (int j=0; j<50; j++)
            arr[i][j] = -1;

    int result = nCr(n, k);

    for (int i=0; i<=n; i++) {
        for (int j=0; j<=k; j++) {
            cout << arr[i][j] << " ";
        }
        cout << endl;
    }

    cout << "Result: " << arr[n][k] << endl;
    return 0;
}
