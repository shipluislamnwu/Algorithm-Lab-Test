#include<iostream>
using namespace std;
int arr[50][50];

int bottom(int n,int k)
{
    if(k==0 || k==n)
    {
        arr[n][k]=1;
        return 1;
    }
    else if (arr[n-1][k-1]!=-1 && arr[n-1][k]!=-1)
    {
        return arr[n][k]=arr[n-1][k-1]+arr[n-1][k];
    }
    else if (arr[n-1][k-1]==-1 && arr[n-1][k]!=-1)
    {
        return arr[n][k]=bottom(n-1,k-1)+arr[n-1][k];
    }
     else if (arr[n-1][k-1]!=-1 && arr[n-1][k]==-1)
     {
       return arr[n][k]=arr[n-1][k-1] + bottom(n-1,k);
     }
     else
     {
         return arr[n][k]=bottom(n-1,k-1)+ bottom(n-1 ,k);
     }
}

int main()
{
    int  k , n , result;
    cin >> n >> k;

    for(int i=0 ; i<50 ; i++)
        for(int j=0 ; j<50 ; j++)
            arr[i][j] = -1;

    result = bottom(n,k);

    for(int i=0 ; i<=n ; i++)
    {
        for(int j=0 ; j<=k; j++)
        {
           cout << arr[i][j] << " ";
        }
        cout << endl;
    }

    cout << "Result: " << arr[n][k];
    return 0;
}
